{
  // Plugin configuration for Semaphore Poll
  // This file contains default settings that can be overridden
  
  // Enable or disable this plugin (default: true)
  "enabled": true,
  
  // Interval in seconds between status checks (default: 10)
  "poll_interval": 10,
  
  // Interval in seconds for periodic "still running" notifications (default: 300)
  // Set to 0 to disable periodic notifications
  "notification_interval": 300
}
